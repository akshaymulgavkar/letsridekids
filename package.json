{
  "name": "letsridekids",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "npm run android && npm run ios",
    "metro": "react-native start",
    "full": "npm install && npm run pod",
    "pod": "cd ios && pod install && cd ..",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && npm install --force && npm run clean:android && npm run clean:ios",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "watchman watch-del-all && cd ios && rm -rf Podfile.lock && rm -rf Pods && pod install && cd ..",
    "build": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "test": "jest",
    "lint": "eslint ."
  },
